{
    "comments": {
        "blockComment": [
            "#|",
            "|#"
        ],
        "lineComment": ";"
    },
    "brackets": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ]
    ],
    "autoClosingPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ],
        [
            "#|",
            "|#"
        ],
        [
            "\"",
            "\""
        ],
    ],
    "surroundingPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
    "colorizedBracketPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ]
    ],
    "onEnterRules": [
        {
            "beforeText": "^\\s*\\(\\s*(define|if\\s|cond\\s|lambda\\s|import\\s|do\\s|unless\\s|library\\s|for-each\\s).*$",
            "action": {
                "indent": "indent"
            }
        },
        {
            "beforeText": "^\\s*;[^;]*$",
            "action": {
                "indent": "none",
                "appendText": ";"
            }
        },
        {
            "beforeText": "^\\s*;;[^;]*$",
            "action": {
                "indent": "none",
                "appendText": ";;"
            }
        },
        {
            "beforeText": "^\\s*;;;[^;]*$",
            "action": {
                "indent": "none",
                "appendText": ";;;"
            }
        },
        {
            "beforeText": "^\\s*;;;;[^;]*$",
            "action": {
                "indent": "none",
                "appendText": ";;;;"
            }
        },
        {
            "beforeText": "^\\s*;;;;;.*$",
            "action": {
                "indent": "none",
                "appendText": ";;;;;"
            }
        },
    ]
}
